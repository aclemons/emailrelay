CONFIG = release
ARCH = x64
BUILD_DIR = .
SRC_DIR = ..
CFLAGS_EXTRA =

!if exist(libresslbuild.cfg)
!include libresslbuild.cfg
!endif

!if "$(CONFIG)" == "release"
SSL_FLAGS_CONFIG = $(SSL_FLAGS_RELEASE)
CRYPTO_FLAGS_CONFIG = $(CRYPTO_FLAGS_RELEASE)
!else
SSL_FLAGS_CONFIG = $(SSL_FLAGS_DEBUG)
CRYPTO_FLAGS_CONFIG = $(CRYPTO_FLAGS_DEBUG)
!endif

.PHONY: all
all: init ssl_lib crypto_lib

.PHONY: init
init:
	-mkdir $(BUILD_DIR:/=\)\library\$(CONFIG) 2>NUL:
	-mkdir $(BUILD_DIR:/=\)\ssl_obj.dir\$(CONFIG) 2>NUL:
	-mkdir $(BUILD_DIR:/=\)\crypto_obj.dir\$(CONFIG) 2>NUL:

.PHONY: ssl_lib
SSL_LIB = $(BUILD_DIR)/library/$(CONFIG)/ssl.lib
ssl_lib: $(SSL_LIB)

.PHONY: crypto_lib
CRYPTO_LIB = $(BUILD_DIR)/library/$(CONFIG)/crypto.lib
crypto_lib: $(CRYPTO_LIB)

