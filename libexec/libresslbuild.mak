$(SSL_LIB): $(SSL_OBJECTS)
	link /lib /nologo /machine:$(ARCH) /out:"$(SSL_LIB)" @<<
		$(SSL_OBJECTS)
<<

$(SSL_OBJECTS):
	cl /nologo /c $(SSL_FLAGS) $(SSL_FLAGS_CONFIG) $(CFLAGS_EXTRA) @<<
		$(SSL_SOURCES)
<<

$(CRYPTO_LIB): $(CRYPTO_OBJECTS)
	link /lib /nologo /machine:$(ARCH) /out:"$(CRYPTO_LIB)" @<<
		$(CRYPTO_OBJECTS)
<<

$(CRYPTO_OBJECTS):
	cl /nologo /c $(CRYPTO_FLAGS) $(CRYPTO_FLAGS_CONFIG) $(CFLAGS_EXTRA) @<<
		$(CRYPTO_SOURCES)
<<

